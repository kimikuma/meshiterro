<h2>プロフィール編集</h2>
<%=form_with model:@user do |f| %>
<label for="InputName">Name</label>
<%=f.text_field:name,autofocus:true,id:"InputName",placeholder:"名前" %>
<label for="InputName">ProfileImage</label>
<%=f.file_field:profile_image,placeholder:"プロフィール編集",accept:"image/*" %>
<%=f.submit '変更を保存'%>
<% end %>